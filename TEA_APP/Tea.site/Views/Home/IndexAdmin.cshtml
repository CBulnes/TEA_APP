@model dynamic
@{
    ViewData["Title"] = "Inicio";
}

<div class="bd-example" style="background-color: #FFF; margin-top: 30px;">
    <div class="row">
        <div class="col-lg-12">
            <div class="col-xs-12 col-sm-6 col-lg-4 col-xl-2">
                <div class="form-group">
                    <label>Año:</label>
                    <select class="input100 custom-select col-12" id="cboAño" style="padding: 0 25px 0 25px;">
                        <option value="2020">2020</option>
                        <option value="2021">2021</option>
                    </select>
                </div>
            </div>
        </div>
    </div>

    <div class="row">

        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Visitas en la web</h4>
                    <div id="visitas" style="width:100%; height:400px;"></div>
                </div>
            </div>
        </div>

        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Solicitudes de contacto</h4>
                    <div id="puntos-visitados" style="width:100%; height:400px;"></div>
                </div>
            </div>
        </div>

    </div>
</div>
